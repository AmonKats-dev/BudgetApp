<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.listviewapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".subcounty.DzaipiHealth"
            android:label="Health" />
        <activity
            android:name=".subcounty.DzaipiSummary"
            android:label="Short Summary" />
        <activity
            android:name=".subcounty.DzaipiSchools"
            android:label="Schools" />
        <activity
            android:name=".subcounty.DzaipiSubCounties"
            android:label="FY 2018/19" />
        <activity android:name=".AdjumaniPrimary" />
        <activity
            android:name=".PopulateActivity"
            android:label="Primary Schools"
            android:parentActivityName=".ExpandableActivity" />
        <activity android:name=".MainPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExpandableActivity"
            android:label="Subcounty"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:label="Local Government"
            android:parentActivityName=".MainPage" />
    </application>

</manifest>